public class Count_Character_Of_String {
    public static void main(String[] args){
        String s = "The best of both worlds" ;
        int count = 0;
        for(int i = 0; i<s.length();i++){
            if(s.charAt(i) != ' '){
                count++;
            }
        }


        System.out.println("Length of string be ");
        System.out.println(s.length());
        System.out.println("Number of characters in string be ");
        System.out.println(count) ;
    }

}
