public class Largest_Element_Of_Array {
    public static void main(String [] args){
        int [] arr = new int [] {1,2,32,4,52} ;
        int temp = 0;
        temp = arr[0] ;
        for(int i = 1; i<arr.length ; i++){
            if(arr[i]>temp){
                temp = arr[i] ;
            }

        }
        System.out.println(" Largest Element of an array be ") ;
        System.out.println(temp) ;
    }
}
