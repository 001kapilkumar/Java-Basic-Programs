public class ReversePyramidPattern {
    public static void main(String [] args){
        int i,j,row =5,k;
        for(i=0;i<row;i++){
            for(j=0;j<=i;j++){
                System.out.print(" ");
            }
            for(k=4;k>=i;k--){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
